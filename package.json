{
  "name": "vessel-management-system",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "frontend",
    "backend"
  ],
  "scripts": {
    "start": "node backend/server.js",
    "build": "cd frontend && npm install && npm run build",
    "dev": "concurrently \"cd backend && npm run dev\" \"cd frontend && npm start\""
  },
  "dependencies": {
    "bcryptjs": "^3.0.2",
    "concurrently": "^8.0.1",
    "dotenv": "^16.4.7",
    "mongoose": "^8.10.2"
  }
}
